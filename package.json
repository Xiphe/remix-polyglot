{
  "name": "remix-polyglot",
  "version": "0.1.0",
  "description": "type-safe, cached-forever, split-able, non-flickering, pre-loading, persistence-agnostic internationalization made for remix â™¥",
  "scripts": {
    "build": "run-p build:**",
    "watch": "run-p watch:*",
    "watch:decl": "npm run build:decl -- --watch",
    "watch:build": "WATCH=1 npm run build",
    "build:cli": "CONFIG=cli node build.js",
    "build:decl": "tsc",
    "build:server:esm": "CONFIG=serverEsm node build.js",
    "build:server:cjs": "CONFIG=serverCjs node build.js",
    "build:client:esm": "CONFIG=clientEsm node build.js",
    "build:client:cjs": "CONFIG=clientCjs node build.js",
    "prepare": "npm run build"
  },
  "main": "dist/server.cjs",
  "module": "dist/server.js",
  "browser": "dist/client.js",
  "types": "dist/server.d.ts",
  "bin": {
    "remix-polyglot": "cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Xiphe/remix-polyglot.git"
  },
  "keywords": [
    "remix",
    "i18n",
    "polyglot",
    "performance",
    "translations",
    "react",
    "types"
  ],
  "bugs": {
    "url": "https://github.com/Xiphe/remix-polyglot/issues"
  },
  "homepage": "https://github.com/Xiphe/remix-polyglot#readme",
  "license": "MIT",
  "peerDependencies": {
    "@remix-run/react": "^1.5.1",
    "jsesc": "^3.0.2",
    "node-polyglot": "^2.4.2",
    "react": ">=16.8"
  },
  "devDependencies": {
    "@remix-run/dev": "1.5.1",
    "@remix-run/eslint-config": "1.5.1",
    "@remix-run/node": "1.5.1",
    "@remix-run/serve": "^1.5.1",
    "@types/jsesc": "3.0.1",
    "@types/node": "17.0.41",
    "@types/node-polyglot": "2.4.2",
    "@types/react": "18.0.12",
    "@types/react-dom": "18.0.5",
    "esbuild": "0.14.43",
    "esbuild-register": "3.3.3",
    "eslint": "8.15.0",
    "npm-run-all": "4.1.5",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "typescript": "4.7.3"
  },
  "dependencies": {
    "cmpl": "0.1.1",
    "ichschwoer": "0.0.1",
    "minimist": "1.2.6",
    "typed-t": "0.1.0"
  }
}
